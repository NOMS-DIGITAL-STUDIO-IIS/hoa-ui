extends ../subject

block subject-details
    div.column-two-thirds
        each val,index in data.details
            div.grid-row.list-row
                div.column-full
                    div.left-border
                        span.quiet.block Created #{val.date.trim()} 
                        span Outcome #{val.outcome}, #{val.outcomeDate ? val.outcomeDate : 'date of outcome unknown'}

            
        if (!data.details)
            span= data.noResultsText