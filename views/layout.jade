doctype html
html
head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(property="og:image" content="/public/images/opengraph-image.png?0.19.2")
    title= content.title


    <!--[if gt IE 8]><!--><link href="/public/stylesheets/govuk-template.css" media="screen" rel="stylesheet" type="text/css" /><!--<![endif]-->
    link(rel="stylesheet", media="print", href="/public/stylesheets/print.css")

    <!--[if gte IE 9]><!--><link href="/public/stylesheets/fonts.css?0.19.2" media="all" rel="stylesheet" /><!--<![endif]-->

    link(rel="shortcut icon", href="/public/images/favicon.ico?0.19.2", type="image/x-icon")
    link(rel="mask-icon", href="/public/images/gov.uk_logotype_crown.svg?0.19.2", color="#0b0c0c")
    link(rel="apple-touch-icon-precomposed", sizes="152x152", href="/public/images/apple-touch-icon-152x152.png?0.19.2")
    link(rel="apple-touch-icon-precomposed", sizes="120x120", href="/public/images/apple-touch-icon-120x120.png?0.19.2")
    link(rel="apple-touch-icon-precomposed", sizes="76x76", href="/public/images/apple-touch-icon-76x76.png?0.19.2")
    link(rel="apple-touch-icon-precomposed", sizes="76x76", href="/public/images/apple-touch-icon-60x60.png?0.19.2")

    link(href="/public/stylesheets/application.css", media="screen", rel="stylesheet", type="text/css")

body

    div(id="skiplink-container")
        div
            a(href="#content", class="skiplink") Skip to main content

    div(id="global-cookie-message")
        p GOV.UK uses cookies to make the site simpler. <a href="#" title="Find out more about cookies">Find out more about cookies

    header(role="banner", id="global-header", class="with-proposition")
        div(class="header-wrapper")
            div(class="header-global")
                div(class="header-logo")
                    a(href="https://www.gov.uk", title="Go to the GOV.UK homepage", id="logo", class="content")
                        img(src="/public/images/gov.uk_logotype_crown_invert_trans.png?0.19.2", width="36", height="32", alt="")
                        span  GOV.UK

            div(class="header-proposition")
                div(class="content")
                    a(href="#proposition-links", class="js-header-toggle menu") Menu
                    nav(id="proposition-menu")
                        a(href="/", id="proposition-name") Inmate information system
                            if nav
                                ul#proposition-links
                                    li
                                        a(href="/change-password") Change Password
                                    li
                                        a(href="/logout") Logout



    <div id="global-header-bar"></div>




    main(id="content", role="main")
        div.phase-banner
            p
                strong.phase-tag ALPHA
                span This is a new service â€“ your <a href="#">feedback</a> will help us to improve it.

        block content

    footer(class="group js-footer", id="footer", role="contentinfo")
        div(class="footer-wrapper")
            div(class="footer-meta")
                div(class="footer-meta-inner")
                    div(class="open-government-licence")
                        p(class="logo")
                            a(href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/", rel="license") Open Government Licence
                        p All content is available under the
                            <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/"  rel="license">Open Government Licence v3.0</a>, except where otherwise stated


                div(class="copyright")
                    a(href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/") &copy; Crown copyright


    div(id="global-app-error", class="app-error hidden")

    script(src="/public/javascripts/govuk-template.js?0.19.2")
    script(src="/public/javascripts/details.polyfill.js")
    script(src="/public/javascripts/jquery-1.11.3.js")
    script(src="/public/javascripts/govuk/selection-buttons.js")
    script(src="/public/javascripts/govuk/shim-links-with-button-role.js")
    script(src="/public/javascripts/govuk/show-hide-content.js")
    script(src="/public/javascripts/application.js")
    
    script.
        document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');